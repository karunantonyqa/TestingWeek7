public class SiteTest {
	
	@Parameters
	public static Collection<Object[]> data(){
		return Arrays.asList(new Object[][] {{10,22},{22,9},{22,22},{10,7},{1,19}});
	}
	
	private int inPut;
	private int intPected;
	
	
	public ExampleTest(int input, int expected) {
		inPut = input;
		intPected = expected;
	}
	
	@Test
	public void test() {
		BlackJack bj = new BlackJack();
		
		assertEquals(intPected, bj.blackJack(inPut, intPected));
		
//		assertEquals(10, bj.blackJack(10,22));
